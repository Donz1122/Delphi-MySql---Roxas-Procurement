  object podetailQ: TMyQuery
    Connection = dataproc.MyConnection1
    SQL.Strings = (
      'select '
      '  a.*'
      'from'
      '('
      '  Select'
      '    ifnull(i.idpodetail,0) as iarIdpodetail,'
      '    po.poNumber,    '
      '    p.* '
      '  from podetail p'
      '  left join iardetail i on i.idpodetail = p.idpodetail'
      '  left join po on po.idpo= p.idpo'
      '  where po.posubmit = 1'
      ''
      ') a'
      'where a.iarIdpodetail = 0 and'
      '      a.poNumber = :poNumber')
    Left = 656
    Top = 48
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'poNumber'
      end>
    object podetailQiarIdpodetail: TLargeintField
      FieldName = 'iarIdpodetail'
    end
    object podetailQpoNumber: TStringField
      FieldName = 'poNumber'
      Size = 25
    end
    object podetailQidPODetail: TLongWordField
      FieldName = 'idPODetail'
    end
    object podetailQidPO: TIntegerField
