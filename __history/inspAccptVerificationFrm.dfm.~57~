  object poDS: TMyDataSource
    DataSet = poQ
    Left = 568
    Top = 48
  end
  object poQ: TMyQuery
    Connection = dataproc.MyConnection1
    SQL.Strings = (
      'select '
      '  a.*'
      'from'
      '('
      '  Select'
      '    ifnull(i.idpodetail,0) as iarIdpodetail,'
      '    po.poNumber,'
      '    p.* '
      '  from podetail p'
      '  left join iardetail i on i.idpodetail = p.idpodetail'
      '  left join po on po.idpo= p.idpo'
      '  where po.posubmit = 1'
      ' '
      ') a'
      'where a.iarIdpodetail <> a.idpodetail'
      'group by a.idpo')
    Left = 529
    Top = 48
    object poQiarIdpodetail: TLargeintField
      FieldName = 'iarIdpodetail'
    end
    object poQpoNumber: TStringField
      FieldName = 'poNumber'
      Size = 25
    end
    object poQidPODetail: TLongWordField
      FieldName = 'idPODetail'
    end
    object poQidPO: TIntegerField
      FieldName = 'idPO'
    end
    object poQidRequisitionDetail: TIntegerField
      FieldName = 'idRequisitionDetail'
    end
    object poQidrequisition: TIntegerField
      FieldName = 'idrequisition'
    end
    object poQidbidding: TIntegerField
      FieldName = 'idbidding'
    end
    object poQdescription: TStringField
