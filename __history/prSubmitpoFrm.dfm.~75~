      VertSpacing = 2
      OnClick = NxLinkLabel2Click
    end
  end
  object biddingQ: TMyQuery
    Connection = dataproc.MyConnection1
    SQL.Strings = (
      'Select '
      '  date_format(sup.dateSumitToPO,'#39'%m/%d/%Y'#39') dateSumitToPO,'
      '  sup.rvnumber,'
      '  sup.pcode,'
      '  sup.payee,'
      '  sup.requester,'
      '  sup.recmmdedDeprtmnt,'
      '  sup.purpose, '
      '  sup.address,'
      '  sup.idrequisition,'
      '  sup.idbidding, '
      '  sum(ifnull(bid.qty,0)*ifnull(bid.cost,0)) amount '
      'from'
      '(select '
      '   a.dateSumitToPO,'
      '   a.rvnumber,'
      '   a.pcode,'
      '   a.payee,'
      '   a.requester,'
      '   a.recmmdedDeprtmnt,'
      '   a.purpose, '
      '   py.address,'
      '   a.idrequisition,'
      '   a.idbidding '
      '  '
      ' from'
      '  ( Select'
      '     r.idrequisition pridReq,'
      '     ifnull(p.idrequisition,0) poidReq,'
      '     ifnull(p.idbidding,0) as poIDbidding,'
      '     r.dateSumitToPO,'
      '     r.requester,   '
      '     r.recmmdedDeprtmnt,'
      '     r.rvdescription purpose,    '
      '     b.* '
      '    from bidding b'
      
        '    left join requisition r on r.idrequisition = b.idrequisition' +
        ' '
      '                             '
      
        '    left join podetail p on p.idrequisition = r.idrequisition an' +
        'd p.idbidding = b.idbidding'
      ''
      '    where b.isWinner =1 and b.isallocated = 0 and'
      '          r.allowallocation = 0'
      '    order by b.idbidding desc'
      '   ) a'
      ' left join payee py on py.pcode = a.pcode'
      ' where a.poIDbidding <> a.idbidding and'
      '       a.pridreq <> a.poidReq'
      ' group by a.idrequisition'
      ') sup'
      'left join bidding bid on bid.iswinner = 1 and'
      '                         bid.idrequisition = sup.idrequisition'
      
        ' where date_format(sup.datesumitToPO,'#39'%Y-%m-%d'#39')  between :dateF' +
        'rom and :dateto and'
      '       sup.payee like :supplier'
      'group by sup.idrequisition,sup.pcode')
    Left = 528
    Top = 48
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'dateFrom'
      end
      item
        DataType = ftUnknown
        Name = 'dateto'
      end
      item
        DataType = ftUnknown
        Name = 'supplier'
      end>
    object biddingQdateSumitToPO: TStringField
      FieldName = 'dateSumitToPO'
      Size = 10
    end
    object biddingQrvnumber: TStringField
      FieldName = 'rvnumber'
    end
    object biddingQpcode: TStringField
      FieldName = 'pcode'
      Size = 30
    end
    object biddingQpayee: TStringField
      FieldName = 'payee'
      Size = 60
    end
    object biddingQrequester: TStringField
      FieldName = 'requester'
      Size = 45
    end
    object biddingQrecmmdedDeprtmnt: TStringField
      FieldName = 'recmmdedDeprtmnt'
      Size = 30
    end
    object biddingQpurpose: TStringField
      FieldName = 'purpose'
      Size = 250
    end
    object biddingQaddress: TStringField
      FieldName = 'address'
      Size = 70
    end
    object biddingQidrequisition: TIntegerField
      FieldName = 'idrequisition'
    end
    object biddingQidbidding: TIntegerField
      FieldName = 'idbidding'
    end
    object biddingQamount: TFloatField
      FieldName = 'amount'
      DisplayFormat = '###,###,###,##0.00'
    end
  end
  object bidDS: TMyDataSource
    DataSet = biddingQ
    Left = 576
    Top = 48
  end
  object reqDetailQ: TMyQuery
    Connection = dataproc.MyConnection1
    SQL.Strings = (
      'Select '
      'b.*,'
      'rd.description,'
      'rd.unit'
      'from bidding b '
      
        'left join requisitiondetail rd on rd.idrequisitiondetail = b.idr' +
        'equisitiondetail'
      'where b.idrequisition = :idrequisition and b.iswinner = 1')
    Left = 624
    Top = 40
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'idrequisition'
      end>
    object reqDetailQidbidding: TIntegerField
      FieldName = 'idbidding'
