  object prQ: TMyQuery
    Connection = dataproc.MyConnection1
    SQL.Strings = (
      'Select r.*,       '
      '       r.rvDate RequestDate,'
      '       r.dateSubmitToBudget DateSubmitToMBO,'
      '       r.dateSubmitToAward DateSubmitToBAC,'
      '       r.dateSumitToPO DateSubmitToPO,'
      '       i.datereceived as DeliveryDate,'
      '       i.DateInsp as DateInspection,'
      '       '
      
        '       if(DATEDIFF(r.dateSubmitToBudget ,r.rvDate)<0,0,DATEDIFF(' +
        'r.dateSubmitToBudget ,r.rvDate)) reqDays,'
      
        '       if(DATEDIFF(r.dateSubmitToAward, r.dateSubmitToBudget)<0,' +
        '0,DATEDIFF(r.dateSubmitToAward, r.dateSubmitToBudget)) mboDays,'
      
        '       if(DATEDIFF(r.dateSumitToPO,r.dateSubmitToAward)<0,0,DATE' +
        'DIFF(r.dateSumitToPO,r.dateSubmitToAward)) bacDays,'
      
        '       if(DATEDIFF(i.datereceived,r.dateSumitToPO)<0,0,DATEDIFF(' +
        'i.datereceived,r.dateSumitToPO)) poDays,'
      
        '       if(DATEDIFF(i.DateInsp,i.datereceived)<0,0,DATEDIFF(i.Dat' +
        'eInsp,i.datereceived)) inspDays'
      'from requisition r'
      'left join podetail pd on pd.idrequisition = r.idrequisition'
      'left join po p on p.idpo = pd.idpo'
      'left join inspacceptance i on i.pono = p.ponumber ')
    Left = 304
    Top = 192
