  object poq: TMyQuery
    Connection = dataproc.MyConnection1
    SQL.Strings = (
      'Select ia.*'
      ' from  '
      '(   Select'
      '       ifnull(i.idpodetail,0) iarIdpodetail,'
      '       po.pcode,'
      '       po.payee,'
      '       po.address,'
      '       po.poNumber,'
      '       po.podate,       '
      '       p.*'
      '     '
      '    from podetail p'
      
        '    left join po on po.idpo = p.idpo                            ' +
        ' '
      '    left join iardetail i on i.idpodetail = p.idpodetail '
      ''
      '    where po.posubmit =1 '
      ') ia'
      
        'where not exists (Select * from iardetail where idpodetail = ia.' +
        'iaridpodetail)  and'
      '      ia.payee like :supplier and'
      '      ia.podate between :datefrom and :dateto'
      '      '
      'group by ia.idpo'
      'order by ia.idpo desc'
      ''
      ''
      '/*Select ia.*'
      ' from  '
      '(   Select'
